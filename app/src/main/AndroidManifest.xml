<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizGame">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".WelcolmeActivity"></activity>
        <activity android:name=".QuizActivity"></activity>
        <activity android:name=".ScoresActivity"></activity>
        <activity android:name=".FormquestionActivity"></activity>
        <activity android:name=".EndgameActivity"></activity>
    </application>

</manifest>